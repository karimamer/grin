grinMain =
  t1 <- store (CInt 1)
  t2 <- store (CInt 10000)
  t3 <- store (Fupto t1 t2)
  t4 <- store (Fsum t3)
  v.0 <- pure (Fsum t3)
  (CInt r') <- case v.0 of
    (Fsum c.0) ->
      sum c.0
  _prim_int_print r'

upto m n =
  v.1 <- fetch m
  (CInt m') <- case v.1 of
    (CInt x'1.1) ->
      pure v.1
  v.2 <- fetch n
  (CInt n') <- case v.2 of
    (CInt x'1.2) ->
      pure v.2
  b' <- _prim_int_gt m' n'
  case b' of
    #True ->
      pure (CNil)
    #False ->
      m1' <- _prim_int_add m' 1
      m1 <- store (CInt m1')
      p <- store (Fupto m1 n)
      pure (CCons m p)

sum l =
  v.3 <- fetch l
  l2 <- case v.3 of
    (Fupto a.3 b.3) ->
      upto a.3 b.3
  ccp.0 <- case l2 of
    (CNil) ->
      pure 0
    (CCons x xs) ->
      v.4 <- fetch x
      (CInt x') <- case v.4 of
        (CInt x'1.4) ->
          pure v.4
      (CInt s') <- sum xs
      _prim_int_add x' s'
  pure (CInt ccp.0)
